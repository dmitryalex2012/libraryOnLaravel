{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel Swagger API documentation for NIX library",
        "contact": {
            "email": "admin@example.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://libraryOnLaravel/api",
            "description": "NIX library Laravel Swagger API server"
        }
    ],
    "paths": {
        "/books": {
            "get": {
                "tags": [
                    "Books"
                ],
                "summary": "Display a listing of the books",
                "operationId": "examplesAll",
                "responses": {
                    "200": {
                        "description": "Everything is fine"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "X-APP-ID": {
                "type": "apiKey",
                "name": "X-APP-ID",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "NIX library",
            "description": "Some example pages"
        }
    ]
}